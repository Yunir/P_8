<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minHeight="300.0" minWidth="700.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
   <children>
      <AnchorPane prefHeight="40.0" prefWidth="200.0">
         <children>
            <HBox alignment="TOP_RIGHT" prefHeight="40.0" prefWidth="700.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
               <children>
                  <Button layoutX="582.0" layoutY="25.0" mnemonicParsing="false" onAction="#refreshScene" text="Refresh">
                     <HBox.margin>
                        <Insets left="7.0" right="7.0" top="15.0" />
                     </HBox.margin>
                  </Button>
                  <!--<Button mnemonicParsing="false" text="Tutorial">
                     <HBox.margin>
                        <Insets left="7.0" right="7.0" top="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#showLogInDialog" text="Log In">
                     <HBox.margin>
                        <Insets left="7.0" right="7.0" top="15.0" />
                     </HBox.margin>
                  </Button>-->
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="40.0" prefWidth="200.0">
         <children>
            <HBox prefHeight="40.0" prefWidth="700.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
               <children>
                  <Label prefWidth="1000.0" text="Projects">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="15.0" right="15.0" top="10.0" />
                     </padding>
                  </Label>
                  <Label prefWidth="1000.0" text="Aims">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <padding>
                        <Insets bottom="5.0" left="15.0" right="15.0" top="10.0" />
                     </padding>
                  </Label>
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="name" prefWidth="800.0">
         <children>
            <HBox prefWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
               <children>
                  <TableView fx:id="projectsTable" onMouseClicked="#openAimsOfProject" prefHeight="450.0" prefWidth="1000.0">
                     <columns>
                        <TableColumn fx:id="nameOfProject" prefWidth="75.0" text="Name" />
                        <TableColumn fx:id="amountOfAims" prefWidth="75.0" text="Amount" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </TableView>
                  <TableView fx:id="aimsTable" onMouseClicked="#unlockButtons" prefHeight="450.0" prefWidth="1000.0">
                     <columns>
                        <TableColumn fx:id="nameOfAim" prefWidth="75.0" text="Name" />
                        <TableColumn fx:id="priorityOfAim" prefWidth="75.0" text="Priority" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </TableView>
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <AnchorPane prefWidth="200.0">
         <children>
            <HBox prefWidth="800.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
               <children>
                  <Pane prefWidth="1000.0">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <children>
                        <HBox prefWidth="340.0">
                           <children>
                              <Button fx:id="PCreate" mnemonicParsing="false" onAction="#showCreateProjectDialog" text="Create">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="PUpdate" mnemonicParsing="false" onAction="#showUpdateProjectDialog" text="Update">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="PDelete" mnemonicParsing="false" text="Delete">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </Pane>
                  <Pane prefWidth="1000.0">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <children>
                        <HBox prefWidth="340.0">
                           <children>
                              <Button fx:id="ACreate" mnemonicParsing="false" onAction="#showCreateAimDialog" text="Create">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="AUpdate" mnemonicParsing="false" text="Update">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="ADelete" mnemonicParsing="false" text="Delete">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </Pane>
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <!--<AnchorPane layoutX="10.0" layoutY="50.0" prefHeight="40.0" prefWidth="200.0">
         <children>
            <Label alignment="TOP_RIGHT" fx:id="Loading" prefWidth="600.0" text="Loading..." textAlignment="RIGHT" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0">
               <font>
                  <Font size="15.0" />
               </font>
               <padding>
                  <Insets left="5.0" top="-10.0" />
               </padding>
            </Label>
         </children>
      </AnchorPane>-->
   </children>
</VBox>
